{
  "name": "esy-python",
  "version": "3.9.1000",
  "description": "CPython packaged for esy",
  "source": "https://www.python.org/ftp/python/3.9.1/Python-3.9.1.tar.xz#sha256:991c3f8ac97992f3d308fefeb03a64db462574eadbff34ce8bc5bb583d9903ff",
  "override": {
    "buildEnv": {},
    "build": ["./configure #{os == 'windows' ? '--host x86_64-w64-mingw32': ''} --prefix #{self.install}", "make"],
    "install": "make install",
    "buildsInSource": true,
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "val": "#{self.lib / 'pkgconfig'}",
        "scope": "global"
      }
    },
    "dependencies": {
      "esy-openssl": "*"
    }
  }
}
